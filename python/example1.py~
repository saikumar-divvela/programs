import sys

'''
The program checks some basic stuff about python language
'''

print("hello world")
print("first python program")
# Immutable -- string, integer and tuples  

print("\n Immutable example")
a= 3
print(a)
print(id(a))
a=a+1
print("After changing a value")
print(a)
print(id(a))

# mutable - dictionary , list etc -- id won't change after changing the values

print("\n Mutable example")
b=[]
print(b)
print(id(b))
b.append(23)
b.append(56)
print(b)
print(id(b))


print("\n\n")
print(sys.maxsize)
print(sys.maxsize+1)
