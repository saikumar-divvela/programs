for i in range (2,5):
    print (i)

vehicles = ["scooter","car","train"]
vehicles.append("zeep")
vehicles.sort()
for index,value in enumerate(vehicles):
    print(index,value);

# iterating dictionary example
print("\n\nIterating over dictionary")
person = {"name": "saikumar","age":34}
for key in person.keys():
    print(key)

for value in person.values():
    print(value)

for key,value in person.items():
    print(key,value)
